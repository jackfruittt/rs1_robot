    Property Tree Widget:
      Expanded:
        - /Global Options1
        - /Status1
        - /TF1
        - /DroneModel1
        - /FrontCamera1
        - /BottomCamera1
      Splitter Ratio: 0.5
    Tree Height: 557
  - Class: rviz_common/Selection
    Name: Selection
  - Class: rviz_common/Tool Properties
    Expanded:
      - /2D Goal Pose1
      - /Publish Point1
