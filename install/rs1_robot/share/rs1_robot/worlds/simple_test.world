    <!-- THIS FILE IS NOW REDUNDANT BECAUSE IGNITION USES SDF NOT WORLD, KEEP INCASE GOING BACK TO CLASSIC -->

<?xml version="1.0" ?>
<sdf version="1.6">
  <world name="simple_test">
    
    <!-- Core system plugins required for igniton-fortress -->
    <plugin filename="gz-sim-physics-system" name="gz::sim::systems::Physics"/>
    <plugin filename="gz-sim-user-commands-system" name="gz::sim::systems::UserCommands"/>
    <plugin filename="gz-sim-scene-broadcaster-system" name="gz::sim::systems::SceneBroadcaster"/>
    
    <!-- Sensors system - required for ANY sensor functionality -->
    <plugin filename="gz-sim-sensors-system" name="gz::sim::systems::Sensors">
      <render_engine>ogre2</render_engine>
    </plugin>
    
    <!-- Global light source -->
    <include>
      <uri>model://sun</uri>
    </include>
    
    <!-- Ground plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>
    
    <!-- Basic objects for the camera to see -->
    <include>
      <uri>model://cardboard_box</uri>
      <name>box1</name>
      <pose>2 0 0.5 0 0 0</pose>
    </include>
    
    <include>
      <uri>model://cardboard_box</uri>
      <name>box2</name>
      <pose>-2 2 0.5 0 0 0</pose>
    </include>
    
    <include>
      <uri>model://cafe_table</uri>
      <name>table1</name>
      <pose>0 3 0 0 0 0</pose>
    </include>
    
    <!-- Physics settings -->
    <physics type="ode">
      <real_time_update_rate>1000.0</real_time_update_rate>
      <max_step_size>0.001</max_step_size>
      <real_time_factor>1</real_time_factor>
      <ode>
        <solver>
          <type>quick</type>
          <iters>150</iters>
          <precon_iters>0</precon_iters>
          <sor>1.400000</sor>
          <use_dynamic_moi_rescaling>1</use_dynamic_moi_rescaling>
        </solver>
        <constraints>
          <cfm>0.00001</cfm>
          <erp>0.2</erp>
          <contact_max_correcting_vel>2000.000000</contact_max_correcting_vel>
          <contact_surface_layer>0.01000</contact_surface_layer>
        </constraints>
      </ode>
    </physics>
    
  </world>
</sdf>
